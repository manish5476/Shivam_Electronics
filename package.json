    "@tailwindcss/postcss": "^4.0.6",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^18.18.0",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
